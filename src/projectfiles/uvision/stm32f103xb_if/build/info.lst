Nvoid info_crc_compute()
N{
N    crc_bootloader = 0;
N    crc_interface = 0;
N    crc_config_admin = 0;
N    crc_config_user = 0;
N
N    // Compute the CRCs of regions that exist
N    if (DAPLINK_ROM_BL_SIZE > 0) {
X    if (0x0000BC00 > 0) {
N        crc_bootloader = crc32((void *)DAPLINK_ROM_BL_START, DAPLINK_ROM_BL_SIZE - 4);
X        crc_bootloader = crc32((void *)0x08000000, 0x0000BC00 - 4);
N    }
N
N    if (DAPLINK_ROM_IF_SIZE > 0) {
X    if (0x00013C00 > 0) {
N        crc_interface = crc32((void *)DAPLINK_ROM_IF_START, DAPLINK_ROM_IF_SIZE - 4);
X        crc_interface = crc32((void *)0x0800C000, 0x00013C00 - 4);
N    }
N
N    if (DAPLINK_ROM_CONFIG_ADMIN_SIZE > 0) {
X    if (0x00000400 > 0) {
N        crc_config_admin = crc32((void *)DAPLINK_ROM_CONFIG_ADMIN_START, DAPLINK_ROM_CONFIG_ADMIN_SIZE);
X        crc_config_admin = crc32((void *)0x0800BC00, 0x00000400);
N    }
N
N    if (DAPLINK_ROM_CONFIG_USER_SIZE > 0) {
X    if (0x00000400 > 0) {
N        crc_config_user = crc32((void *)DAPLINK_ROM_CONFIG_USER_START, DAPLINK_ROM_CONFIG_USER_SIZE);
X        crc_config_user = crc32((void *)0x0801FC00, 0x00000400);
N    }
N}
