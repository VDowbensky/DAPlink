N#define JTAG_SEQUENCE_TMS               0x40U   // TMS value
N#define JTAG_SEQUENCE_TDO               0x80U   // TDO capture
N
N// SWD Sequence Info
N#define SWD_SEQUENCE_CLK                0x3FU   // SWCLK count
N#define SWD_SEQUENCE_DIN                0x80U   // SWDIO capture
