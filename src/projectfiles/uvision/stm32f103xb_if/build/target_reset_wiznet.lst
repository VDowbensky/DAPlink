N#define DP_SELECT                       0x08U   // Select Register (JTAG R/W & SW W)
N#define DP_RESEND                       0x08U   // Resend (SW Read Only)
N#define DP_RDBUFF                       0x0CU   // Read Buffer (Read Only)
