Nstatic uint8_t target_set_state_wiznet(target_state_t state) {
N    uint8_t status;
N
N    if( state == RESET_RUN )
N    {
N        do
N        {
N            swd_set_target_reset(1);
N            osDelay(2);
N            swd_set_target_reset(0);
N            osDelay(2);
N        } while(!swd_init_debug());
N
N        swd_off();
N        status = 1;
N    }
N    else
N    {
N        status = swd_set_target_state_sw(state);
N    }
N
N    return status;
N}
