S__STATIC_FORCEINLINE void PIN_DELAY_SLOW (uint32_t delay) {
S  __ASM volatile (
S  ".syntax unified\n"
S  "0:\n\t"
S    "subs %0,%0,#1\n\t"
S    "bne  0b\n"
S  : "+l" (delay) : : "cc"
S  );
