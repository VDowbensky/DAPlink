N#define DAP_TRANSFER_WAIT               (1U<<1)
N#define DAP_TRANSFER_FAULT              (1U<<2)
N#define DAP_TRANSFER_ERROR              (1U<<3)
N#define DAP_TRANSFER_MISMATCH           (1U<<4)
