N
N    for (; i < (record->byte_count + 5); i++) {
N        result += record->buf[i];
N    }
N
N    return (result == 0);
N}
N
Nstatic hex_line_t line = {0}, shadow_line = {0};
Nstatic uint32_t next_address_to_write = 0;
Nstatic uint8_t low_nibble = 0, idx = 0, record_processed = 0, load_unaligned_record = 0;
N
Nvoid reset_hex_parser(void)
N{
N    memset(line.buf, 0, sizeof(hex_line_t));
N    memset(shadow_line.buf, 0, sizeof(hex_line_t));
N    next_address_to_write = 0;
N    low_nibble = 0;
N    idx = 0;
N    record_processed = 0;
N    load_unaligned_record = 0;
N}
N
Nhexfile_parse_status_t parse_hex_blob(const uint8_t *hex_blob, const uint32_t hex_blob_size, uint32_t *hex_parse_cnt, uint8_t *bin_buf, const uint32_t bin_buf_size, uint32_t *bin_buf_address, uint32_t *bin_buf_cnt)
N{
N    uint8_t *end = (uint8_t *)hex_blob + hex_blob_size;
N    hexfile_parse_status_t status = HEX_PARSE_UNINIT;
N    // reset the amount of data that is being return'd
N    *bin_buf_cnt = (uint32_t)0;
N
N    // we had an exit state where the address was unaligned to the previous record and data count.
N    //  Need to pop the last record into the buffer before decoding anthing else since it was
N    //  already decoded.
N    if (load_unaligned_record) {
N        // need some help...
N        load_unaligned_record = 0;
N        // move from line buffer back to input buffer
N        memcpy((uint8_t *)bin_buf, (uint8_t *)line.data, line.byte_count);
N        bin_buf += line.byte_count;
N        *bin_buf_cnt = (uint32_t)(*bin_buf_cnt) + line.byte_count;
N        // Store next address to write
N        next_address_to_write = ((next_address_to_write & 0xffff0000) | line.address) + line.byte_count;
N    }
N
N    while (hex_blob != end) {
N        switch ((uint8_t)(*hex_blob)) {
N            // we've hit the end of an ascii line
N            // junk we dont care about could also just run the validate_checksum on &line
N            case '\r':
N            case '\n':
N                //ignore new lines
N                break;
N
N            // found start of a new record. reset state variables
N            case ':':
N                memset(line.buf, 0, sizeof(hex_line_t));
N                low_nibble = 0;
N                idx = 0;
N                record_processed = 0;
N                break;
N
N            // decoding lines
N            default:
N                if (low_nibble) {
N                    line.buf[idx] |= ctoh((uint8_t)(*hex_blob)) & 0xf;
N                    if (++idx >= (line.byte_count + 5)) { //all data in
N                        if (0 == validate_checksum(&line)) {
N                            status = HEX_PARSE_CKSUM_FAIL;
N                            goto hex_parser_exit;
N                        } else {
N                            if (!record_processed) {
N                                record_processed = 1;
N                                // address byteswap...
N                                line.address = swap16(line.address);
N
N                                switch (line.record_type) {
N                                    case DATA_RECORD:
N                                        // keeping a record of the last hex record
N                                        memcpy(shadow_line.buf, line.buf, sizeof(hex_line_t));
N
N                                        // verify this is a continous block of memory or need to exit and dump
N                                        if (((next_address_to_write & 0xffff0000) | line.address) != next_address_to_write) {
N                                            load_unaligned_record = 1;
N                                            status = HEX_PARSE_UNALIGNED;
N                                            goto hex_parser_exit;
N                                        }
N
N                                        // move from line buffer back to input buffer
N                                        memcpy(bin_buf, line.data, line.byte_count);
N                                        bin_buf += line.byte_count;
N                                        *bin_buf_cnt = (uint32_t)(*bin_buf_cnt) + line.byte_count;
N                                        // Save next address to write
N                                        next_address_to_write = ((next_address_to_write & 0xffff0000) | line.address) + line.byte_count;
N                                        break;
N
N                                    case EOF_RECORD:
N                                        status = HEX_PARSE_EOF;
N                                        goto hex_parser_exit;
N
N                                    case EXT_SEG_ADDR_RECORD:
N                                        // Could have had data in the buffer so must exit and try to program
N                                        //  before updating bin_buf_address with next_address_to_write
N                                        memset(bin_buf, 0xff, (bin_buf_size - (uint32_t)(*bin_buf_cnt)));
N                                        // figure the start address for the buffer before returning
N                                        *bin_buf_address = next_address_to_write - (uint32_t)(*bin_buf_cnt);
N                                        *hex_parse_cnt = (uint32_t)(hex_blob_size - (end - hex_blob));
N                                        // update the address msb's
N                                        next_address_to_write = (next_address_to_write & 0x00000000) | ((line.data[0] << 12) | (line.data[1] << 4));
N                                        // Need to exit and program if buffer has been filled
N                                        status = HEX_PARSE_UNALIGNED;
N                                        return status;
N
N                                    case EXT_LINEAR_ADDR_RECORD:
N                                        // Could have had data in the buffer so must exit and try to program
N                                        //  before updating bin_buf_address with next_address_to_write
N                                        //  Good catch Gaute!!
N                                        memset(bin_buf, 0xff, (bin_buf_size - (uint32_t)(*bin_buf_cnt)));
N                                        // figure the start address for the buffer before returning
N                                        *bin_buf_address = next_address_to_write - (uint32_t)(*bin_buf_cnt);
N                                        *hex_parse_cnt = (uint32_t)(hex_blob_size - (end - hex_blob));
N                                        // update the address msb's
N                                        next_address_to_write = (next_address_to_write & 0x00000000) | ((line.data[0] << 24) | (line.data[1] << 16));
N                                        // Need to exit and program if buffer has been filled
N                                        status = HEX_PARSE_UNALIGNED;
N                                        return status;
N
N                                    default:
N                                        break;
N                                }
N                            }
N                        }
N                    }
N                } else {
N                    if (idx < sizeof(hex_line_t)) {
N                        line.buf[idx] = ctoh((uint8_t)(*hex_blob)) << 4;
N                    }
N                }
N
N                low_nibble = !low_nibble;
N                break;
N        }
N
N        hex_blob++;
N    }
N
N    // decoded an entire hex block - verify (cant do this hex_parse_cnt is figured below)
N    //status = (hex_blob_size == (uint32_t)(*hex_parse_cnt)) ? HEX_PARSE_OK : HEX_PARSE_FAILURE;
N    status = HEX_PARSE_OK;
Nhex_parser_exit:
N    memset(bin_buf, 0xff, (bin_buf_size - (uint32_t)(*bin_buf_cnt)));
N    // figure the start address for the buffer before returning
N    *bin_buf_address = next_address_to_write - (uint32_t)(*bin_buf_cnt);
N    *hex_parse_cnt = (uint32_t)(hex_blob_size - (end - hex_blob));
N    return status;
N}
