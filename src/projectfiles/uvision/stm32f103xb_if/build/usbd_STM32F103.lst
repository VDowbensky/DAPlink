N/*
N *  USB Device Interrupt Service Routine
N */
N
Nvoid USB_LP_CAN1_RX0_IRQHandler(void)
N{
N    uint32_t istr;
N    uint32_t num;
N    uint32_t val;
N
N    istr = ISTR;
X    istr = (*((volatile unsigned int *)(0x40005C00 + 0x44)));
N    // Zero out endpoint ID since this is read from the queue
N    LastIstr |= istr & ~(ISTR_DIR | ISTR_EP_ID);
X    LastIstr |= istr & ~(0x0010 | 0x000F);
N    // Clear interrupts that are pending
N    ISTR = ~(istr & USB_ISTR_W0C_MASK);
X    (*((volatile unsigned int *)(0x40005C00 + 0x44))) = ~(istr & (0x4000 | 0x2000 | 0x1000 | 0x0800 | 0x0400 | 0x0200 | 0x0100));
N    if (istr & ISTR_CTR) {
X    if (istr & 0x8000) {
N        while ((istr = ISTR) & ISTR_CTR) {
X        while ((istr = (*((volatile unsigned int *)(0x40005C00 + 0x44)))) & 0x8000) {
N            num = istr & ISTR_EP_ID;
X            num = istr & 0x000F;
N            val = EPxREG(num);
X            val = (*((volatile unsigned int *)(0x40005C00 + 4*(num))));
N
N            // Process and filter out the zero length status out endpoint to prevent
N            // the next SETUP packet from being dropped.
N            if ((0 == num) && (val & EP_CTR_RX) && !(val & EP_SETUP)
X            if ((0 == num) && (val & 0x8000) && !(val & 0x0800)
N                    && (0 == ((pBUF_DSCR + num)->COUNT_RX & EP_COUNT_MASK))) {
X                    && (0 == ((pBUF_DSCR + num)->COUNT_RX & 0x03FF))) {
N                if (val & EP_CTR_TX) {
X                if (val & 0x0080) {
N                    // Drop the RX event but not TX
N                    stat_enque((((val & VAL_MASK) & ~EP_CTR_RX) << VAL_SHIFT) | 
X                    stat_enque((((val & 0xFFFF) & ~0x8000) << 16) | 
N                               ((num & EP_NUM_MASK) << EP_NUM_SHIFT));
X                               ((num & 0xFFFF) << 0));
N                } else {
N                    // Drop the event
N                }
N            } else {
N                stat_enque(((val & VAL_MASK) << VAL_SHIFT) | 
X                stat_enque(((val & 0xFFFF) << 16) | 
N                           ((num & EP_NUM_MASK) << EP_NUM_SHIFT));
X                           ((num & 0xFFFF) << 0));
N            }
N
N
N            if (val & EP_CTR_RX) {
X            if (val & 0x8000) {
N                EPxREG(num) = EP_VAL_UNCHANGED(val) & ~EP_CTR_RX;
X                (*((volatile unsigned int *)(0x40005C00 + 4*(num)))) = (((val) & (0x8000|0x0800|0x0600|0x0100|0x0080|0x000F)) | (0x8000|0x0080)) & ~0x8000;
N            }
N
N            if (val & EP_CTR_TX) {
X            if (val & 0x0080) {
N                EPxREG(num) = EP_VAL_UNCHANGED(val) & ~EP_CTR_TX;
X                (*((volatile unsigned int *)(0x40005C00 + 4*(num)))) = (((val) & (0x8000|0x0800|0x0600|0x0100|0x0080|0x000F)) | (0x8000|0x0080)) & ~0x0080;
N            }
N        }
N    }
N    
N    USBD_SignalHandler();
N}
N
Nvoid USBD_Handler(void)
N{
N    U32 istr, num, val, num_val;
N    cortex_int_state_t state;
N
N    // Get ISTR
N    state = cortex_int_get_and_disable();
N    istr = LastIstr;
N    LastIstr = 0;
N    cortex_int_restore(state);
N
N    /* USB Reset Request                                                        */
N    if (istr & ISTR_RESET) {
X    if (istr & 0x0400) {
N        USBD_Reset();
N        usbd_reset_core();
N#ifdef __RTX
S
S        if (USBD_RTX_DevTask) {
S            isr_evt_set(USBD_EVT_RESET, USBD_RTX_DevTask);
S        }
S
N#else
N
N        if (USBD_P_Reset_Event) {
N            USBD_P_Reset_Event();
N        }
N
N#endif
N    }
N
N    /* USB Suspend Request                                                      */
N    if (istr & ISTR_SUSP) {
X    if (istr & 0x0800) {
N        USBD_Suspend();
N#ifdef __RTX
S
S        if (USBD_RTX_DevTask) {
S            isr_evt_set(USBD_EVT_SUSPEND, USBD_RTX_DevTask);
S        }
S
N#else
N
N        if (USBD_P_Suspend_Event) {
N            USBD_P_Suspend_Event();
N        }
N
N#endif
N    }
N
N    /* USB Wakeup                                                               */
N    if (istr & ISTR_WKUP) {
X    if (istr & 0x1000) {
N        USBD_WakeUp();
N#ifdef __RTX
S
S        if (USBD_RTX_DevTask) {
S            isr_evt_set(USBD_EVT_RESUME,  USBD_RTX_DevTask);
S        }
S
N#else
N
N        if (USBD_P_Resume_Event) {
N            USBD_P_Resume_Event();
N        }
N
N#endif
N    }
N
N    /* Start of Frame                                                           */
N    if (istr & ISTR_SOF) {
X    if (istr & 0x0200) {
N#ifdef __RTX
S
S        if (USBD_RTX_DevTask) {
S            isr_evt_set(USBD_EVT_SOF, USBD_RTX_DevTask);
S        }
S
N#else
N
N        if (USBD_P_SOF_Event) {
N            USBD_P_SOF_Event();
N        }
N
N#endif
N    }
N
N    /* PMA Over/underrun                                                        */
N    if (istr & ISTR_PMAOVR) {
X    if (istr & 0x4000) {
N#ifdef __RTX
S        LastError = 2;
S
S        if (USBD_RTX_DevTask) {
S            isr_evt_set(USBD_EVT_ERROR, USBD_RTX_DevTask);
S        }
S
N#else
N
N        if (USBD_P_Error_Event) {
N            USBD_P_Error_Event(2);
N        }
N
N#endif
N    }
N
N    /* Error: No Answer, CRC Error, Bit Stuff Error, Frame Format Error         */
N    if (istr & ISTR_ERR) {
X    if (istr & 0x2000) {
N#ifdef __RTX
S        LastError = 1;
S
S        if (USBD_RTX_DevTask) {
S            isr_evt_set(USBD_EVT_ERROR, USBD_RTX_DevTask);
S        }
S
N#else
N
N        if (USBD_P_Error_Event) {
N            USBD_P_Error_Event(1);
N        }
N
N#endif
N    }
N
N    /* Endpoint Interrupts                                                      */
N    while ((istr & ISTR_CTR) && !stat_is_empty()) {
X    while ((istr & 0x8000) && !stat_is_empty()) {
N        num_val = stat_deque();
N        num = (num_val >> EP_NUM_SHIFT) & EP_NUM_MASK;
X        num = (num_val >> 0) & 0xFFFF;
N        val = (num_val >> VAL_SHIFT) & VAL_MASK;
X        val = (num_val >> 16) & 0xFFFF;
N        if (val & EP_CTR_TX) {
X        if (val & 0x0080) {
N#ifdef __RTX
S
S            if (USBD_RTX_EPTask[num]) {
S                isr_evt_set(USBD_EVT_IN,  USBD_RTX_EPTask[num]);
S            }
S
N#else
N
N            if (USBD_P_EP[num]) {
N                USBD_P_EP[num](USBD_EVT_IN);
X                USBD_P_EP[num]((1 << 3));
N            }
N
N#endif
N        }
N
N        if (val & EP_CTR_RX) {
X        if (val & 0x8000) {
N#ifdef __RTX
S
S            if (USBD_RTX_EPTask[num]) {
S                isr_evt_set((val & EP_SETUP) ? USBD_EVT_SETUP : USBD_EVT_OUT, USBD_RTX_EPTask[num]);
S            }
S
N#else
N
N            if (USBD_P_EP[num]) {
N                USBD_P_EP[num]((val & EP_SETUP) ? USBD_EVT_SETUP : USBD_EVT_OUT);
X                USBD_P_EP[num]((val & 0x0800) ? (1 << 1) : (1 << 2));
N            }
N
N#endif
N        }
N    }
N}
