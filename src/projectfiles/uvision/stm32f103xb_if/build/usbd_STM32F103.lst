N/*----------------------------------------------------------------------------
N *      RL-ARM - USB
N *----------------------------------------------------------------------------
N *      Name:    usbreg.h
N *      Purpose: Hardware Layer Definitions for ST STM32F10x
N *---------------------------------------------------------------------------*/
N
N#ifndef __USBREG_H
N#define __USBREG_H
N
N
N#define REG(x)  (*((volatile unsigned int *)(x)))
N
N#define USB_BASE_ADDR   0x40005C00  /* USB Registers Base Address */
N#define USB_PMA_ADDR    0x40006000  /* USB Packet Memory Area Address */
N
N
N/* Common Registers */
N#define CNTR    REG(USB_BASE_ADDR + 0x40)   /* Control Register */
N#define ISTR    REG(USB_BASE_ADDR + 0x44)   /* Interrupt Status Register */
N#define FNR     REG(USB_BASE_ADDR + 0x48)   /* Frame Number Register */
N#define DADDR   REG(USB_BASE_ADDR + 0x4C)   /* Device Address Register */
N#define BTABLE  REG(USB_BASE_ADDR + 0x50)   /* Buffer Table Address Register */
N
N/* CNTR: Control Register Bit Definitions */
N#define CNTR_CTRM       0x8000      /* Correct Transfer Interrupt Mask */
N#define CNTR_PMAOVRM    0x4000      /* Packet Memory Aerea Over/underrun Interrupt Mask */
N#define CNTR_ERRM       0x2000      /* Error Interrupt Mask */
N#define CNTR_WKUPM      0x1000      /* Wake-up Interrupt Mask */
N#define CNTR_SUSPM      0x0800      /* Suspend Mode Interrupt Mask  */
N#define CNTR_RESETM     0x0400      /* USB Reset Interrupt Mask   */
N#define CNTR_SOFM       0x0200      /* Start of Frame Interrupt Mask */
N#define CNTR_ESOFM      0x0100      /* Expected Start of Frame Interrupt Mask */
N#define CNTR_RESUME     0x0010      /* Resume Request */
N#define CNTR_FSUSP      0x0008      /* Force Suspend */
N#define CNTR_LPMODE     0x0004      /* Low-power Mode */
N#define CNTR_PDWN       0x0002      /* Power Down */
N#define CNTR_FRES       0x0001      /* Force USB Reset */
N
N/* ISTR: Interrupt Status Register Bit Definitions */
N#define ISTR_CTR        0x8000      /* Correct Transfer */
N#define ISTR_PMAOVR     0x4000      /* Packet Memory Aerea Over/underrun */
N#define ISTR_ERR        0x2000      /* Error */
N#define ISTR_WKUP       0x1000      /* Wake-up */
N#define ISTR_SUSP       0x0800      /* Suspend Mode */
N#define ISTR_RESET      0x0400      /* USB Reset */
N#define ISTR_SOF        0x0200      /* Start of Frame */
N#define ISTR_ESOF       0x0100      /* Expected Start of Frame */
N#define ISTR_DIR        0x0010      /* Direction of Transaction */
N#define ISTR_EP_ID      0x000F      /* EndPoint Identifier */
N
N/* FNR: Frame Number Register Bit Definitions */
N#define FNR_RXDP        0x8000      /* D+ Data Line Status */
N#define FNR_RXDM        0x4000      /* D- Data Line Status */
N#define FNR_LCK         0x2000      /* Locked */
N#define FNR_LSOF        0x1800      /* Lost SOF */
N#define FNR_FN          0x07FF      /* Frame Number */
N
N/* DADDR: Device Address Register Bit Definitions */
N#define DADDR_EF        0x0080      /* Enable Function */
N#define DADDR_ADD       0x007F      /* Device Address */
N
N
N/* EndPoint Registers */
N#define EPxREG(x)       REG(USB_BASE_ADDR + 4*(x))
N
N/* EPxREG: EndPoint Registers Bit Definitions */
N#define EP_CTR_RX       0x8000      /* Correct RX Transfer */
N#define EP_DTOG_RX      0x4000      /* RX Data Toggle */
N#define EP_STAT_RX      0x3000      /* RX Status */
N#define EP_SETUP        0x0800      /* EndPoint Setup */
N#define EP_TYPE         0x0600      /* EndPoint Type */
N#define EP_KIND         0x0100      /* EndPoint Kind */
N#define EP_CTR_TX       0x0080      /* Correct TX Transfer */
N#define EP_DTOG_TX      0x0040      /* TX Data Toggle */
N#define EP_STAT_TX      0x0030      /* TX Status */
N#define EP_EA           0x000F      /* EndPoint Address */
N
N/* EndPoint Register Mask (No Toggle Fields) */
N#define EP_MASK         (EP_CTR_RX|EP_SETUP|EP_TYPE|EP_KIND|EP_CTR_TX|EP_EA)
N/* EndPoint Register Mask (Write zero to clear) */
N#define EP_MASK_RC_W0   (EP_CTR_RX|EP_CTR_TX)
N/* Mask off all toggle bits and set write zero to clear bits to 1.          */
N/* This creates a value that can be written back to the EndPoint register   */
N/* which does not change any status bits.                                   */
N#define EP_VAL_UNCHANGED(val)       (((val) & EP_MASK) | EP_MASK_RC_W0)
N
N/* EP_TYPE: EndPoint Types */
N#define EP_BULK         0x0000      /* BULK EndPoint */
N#define EP_CONTROL      0x0200      /* CONTROL EndPoint */
N#define EP_ISOCHRONOUS  0x0400      /* ISOCHRONOUS EndPoint */
N#define EP_INTERRUPT    0x0600      /* INTERRUPT EndPoint */
N
N/* EP_KIND: EndPoint Kind */
N#define EP_DBL_BUF      EP_KIND     /* Double Buffer for Bulk Endpoint */
N#define EP_STATUS_OUT   EP_KIND     /* Status Out for Control Endpoint */
N
N/* EP_STAT_TX: TX Status */
N#define EP_TX_DIS       0x0000      /* Disabled */
N#define EP_TX_STALL     0x0010      /* Stalled */
N#define EP_TX_NAK       0x0020      /* NAKed */
N#define EP_TX_VALID     0x0030      /* Valid */
N
N/* EP_STAT_RX: RX Status */
N#define EP_RX_DIS       0x0000      /* Disabled */
N#define EP_RX_STALL     0x1000      /* Stalled */
N#define EP_RX_NAK       0x2000      /* NAKed */
N#define EP_RX_VALID     0x3000      /* Valid */
N
N
N/* Endpoint Buffer Descriptor */
Ntypedef struct _EP_BUF_DSCR {
N    U32 ADDR_TX;
N    U32 COUNT_TX;
N    U32 ADDR_RX;
N    U32 COUNT_RX;
N} EP_BUF_DSCR;
N
N#define EP_ADDR_MASK    0xFFFE      /* Address Mask */
N#define EP_COUNT_MASK   0x03FF      /* Count Mask */
N
N
N#endif  /* __USBREG_H */
