N/*
N *  Read USB Device Endpoint Data
N *    Parameters:      EPNum: Device Endpoint Number
N *                       EPNum.0..3: Address
N *                       EPNum.7:    Dir
N *                     pData: Pointer to Data Buffer
N *    Return Value:    Number of bytes read
N */
N
NU32 USBD_ReadEP(U32 EPNum, U8 *pData, U32 bufsz)
N{
N    /* Double Buffering is not yet supported                                    */
N    U32 num, cnt, *pv, n;
N    num = EPNum & 0x0F;
N    pv  = (U32 *)(USB_PMA_ADDR + 2 * ((pBUF_DSCR + num)->ADDR_RX));
X    pv  = (U32 *)(0x40006000 + 2 * ((pBUF_DSCR + num)->ADDR_RX));
N    cnt = (pBUF_DSCR + num)->COUNT_RX & EP_COUNT_MASK;
X    cnt = (pBUF_DSCR + num)->COUNT_RX & 0x03FF;
N    if (cnt > bufsz) {
N        cnt = bufsz;
N    }
N
N    for (n = 0; n < (cnt + 1) / 2; n++) {
N        *((__packed U16 *)pData) = *pv++;
X        *((__packed U16 *)pData) = *pv++;
N        pData += 2;
N    }
N
N    EP_Status(EPNum, EP_RX_VALID);
X    EP_Status(EPNum, 0x3000);
N    return (cnt);
N}
N
