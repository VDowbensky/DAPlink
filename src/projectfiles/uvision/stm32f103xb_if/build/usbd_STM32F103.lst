N/*
N *  Configure USB Device Endpoint according to Descriptor
N *    Parameters:      pEPD:  Pointer to Device Endpoint Descriptor
N *    Return Value:    None
N */
N
Nvoid USBD_ConfigEP(USB_ENDPOINT_DESCRIPTOR *pEPD)
N{
N    /* Double Buffering is not yet supported                                    */
N    U32 num, val;
N    num = pEPD->bEndpointAddress & 0x0F;
N    val = pEPD->wMaxPacketSize;
N
N    if (pEPD->bEndpointAddress & USB_ENDPOINT_DIRECTION_MASK) {
X    if (pEPD->bEndpointAddress & 0x80) {
N        (pBUF_DSCR + num)->ADDR_TX = FreeBufAddr;
N        val = (val + 1) & ~1;
N    } else {
N        (pBUF_DSCR + num)->ADDR_RX = FreeBufAddr;
N
N        if (val > 62) {
N            val = (val + 31) & ~31;
N            (pBUF_DSCR + num)->COUNT_RX = ((val << 5) - 1) | 0x8000;
N        } else {
N            val = (val + 1)  & ~1;
N            (pBUF_DSCR + num)->COUNT_RX =   val << 9;
N        }
N    }
N
N    FreeBufAddr += val;
N
N    switch (pEPD->bmAttributes & USB_ENDPOINT_TYPE_MASK) {
X    switch (pEPD->bmAttributes & 0x03) {
N        case USB_ENDPOINT_TYPE_CONTROL:
X        case 0x00:
N            val = EP_CONTROL;
X            val = 0x0200;
N            break;
N
N        case USB_ENDPOINT_TYPE_ISOCHRONOUS:
X        case 0x01:
N            val = EP_ISOCHRONOUS;
X            val = 0x0400;
N            break;
N
N        case USB_ENDPOINT_TYPE_BULK:
X        case 0x02:
N            val = EP_BULK;
X            val = 0x0000;
N
N            if (USB_DBL_BUF_EP & (1 << num)) {
X            if (0x0000 & (1 << num)) {
N                val |= EP_KIND;
X                val |= 0x0100;
N            }
N
N            break;
N
N        case USB_ENDPOINT_TYPE_INTERRUPT:
X        case 0x03:
N            val = EP_INTERRUPT;
X            val = 0x0600;
N            break;
N    }
N
N    val |= num;
N    EPxREG(num) = val;
X    (*((volatile unsigned int *)(0x40005C00 + 4*(num)))) = val;
N}
