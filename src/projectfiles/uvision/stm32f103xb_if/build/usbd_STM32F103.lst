N/*
N *  Write USB Device Endpoint Data
N *    Parameters:      EPNum: Device Endpoint Number
N *                       EPNum.0..3: Address
N *                       EPNum.7:    Dir
N *                     pData: Pointer to Data Buffer
N *                     cnt:   Number of bytes to write
N *    Return Value:    Number of bytes written
N */
N
NU32 USBD_WriteEP(U32 EPNum, U8 *pData, U32 cnt)
N{
N    /* Double Buffering is not yet supported                                    */
N    U32 num, *pv, n;
N    U16 statusEP;
N    num = EPNum & 0x0F;
N    pv  = (U32 *)(USB_PMA_ADDR + 2 * ((pBUF_DSCR + num)->ADDR_TX));
X    pv  = (U32 *)(0x40006000 + 2 * ((pBUF_DSCR + num)->ADDR_TX));
N
N    for (n = 0; n < (cnt + 1) / 2; n++) {
N        *pv++ = *((__packed U16 *)pData);
X        *pv++ = *((__packed U16 *)pData);
N        pData += 2;
N    }
N
N    (pBUF_DSCR + num)->COUNT_TX = cnt;
N    statusEP = EPxREG(num);
X    statusEP = (*((volatile unsigned int *)(0x40005C00 + 4*(num))));
N
N    if ((statusEP & EP_STAT_TX) != EP_TX_STALL) {
X    if ((statusEP & 0x0030) != 0x0010) {
N        EP_Status(EPNum, EP_TX_VALID);      /* do not make EP valid if stalled    */
X        EP_Status(EPNum, 0x0030);       
N    }
N
N    return (cnt);
N}
